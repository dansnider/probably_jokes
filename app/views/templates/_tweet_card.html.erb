<a ng-href="{{tweet.comedian_url}}">
	<img class="b-card__comedian_image" ng-src="{{tweet.comedian_image_url}}" />
</a>
<a ng-href="{{tweet.comedian_url}}">
	<h2 class="b-card__comedian_name" ng-bind="tweet.comedian_screen_name"></h2>
</a>
<p class="b-card__tweet_body" ng-bind-html="tweet.linkified_text"></p>
<img class="b-card__tweet_image" ng-src="{{tweet.media}}" />
<br>
<button class="b-card__button--up" ng-click="upVote()"> UP </button>
<button class="b-card__button--down" ng-click="nextTweet()"> NEXT </button>
<button class="b-card__button--share" ng-click="twitterShare()">Twitter</button>